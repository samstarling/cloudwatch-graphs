{
    "awsRegion": "eu-west-1",
    "hoursToShow": 168,
    "metrics": [
        {
            "metric": "running_reads",
            "namespace": "owlim/triple-store-live",
            "title": "Running Reads",
            "type": "gauge"
        },
        {
            "metric": "completed_reads",
            "namespace": "owlim/triple-store-live",
            "title": "Completed Reads",
            "type": "counter"
        },
        {
            "metric": "running_writes",
            "namespace": "owlim/triple-store-live",
            "title": "Running Writes",
            "type": "gauge"
        },
        {
            "dimensions": [
                {
                    "Name": "Environment",
                    "Value": "int"
                },
                {
                    "Name": "Zenoss",
                    "Value": "BBCApp"
                },
                {
                    "Name": "valueType",
                    "Value": "GAUGE"
                }
            ],
            "metric": "2xxStatuses",
            "namespace": "BBCApp/LDM",
            "title": "LDM 2xx",
            "type": "gauge"
        }
